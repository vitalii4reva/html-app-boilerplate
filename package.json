{
  "name": "html-app-template",
  "version": "1.0.0",
  "description": "boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "precommit": "gulp lint && lint-staged",
    "lint": "gulp lint && eslint --ignore-pattern 'app/js/lib/' --cache --format=node_modules/eslint-formatter-pretty app/scripts/",
    "lint:fix": "npm run lint -- --fix",
    "deploy": "gulp build && gulp ftp"
  },
  "lint-staged": {
    "app/**/*.html": [
      "htmlhint --config .htmlhintrc"
    ]
  },
